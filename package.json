{
  "name": "graphql-starred-repositories",
  "private": true,
  "scripts": {
    "build": "parcel build index.html",
    "start": "parcel index.html --open",
    "lint": "yarn eslint",
    "format": "yarn eslint --fix && yarn stylelint --fix && yarn prettier --write",
    "eslint": "eslint --cache './**/*.{ts,tsx}'",
    "prettier": "prettier './**/*.{md,js,ts,tsx,json}'",
    "stylelint": "stylelint --cache --syntax css-in-js './src/**/*.{ts,tsx}'",
    "graphql-codegen": "graphql-codegen --config codegen.yml --require dotenv/config",
    "postinstall": "yarn graphql-codegen"
  },
  "dependencies": {
    "graphql-tag": "2.10.1",
    "lens.ts": "https://github.com/tbo/lens.ts#master",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "urql": "1.8.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.3",
    "@graphql-codegen/fragment-matcher": "5.0.2",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/typescript": "4.1.0",
    "@graphql-codegen/typescript-operations": "4.3.0",
    "@graphql-codegen/typescript-urql": "4.0.0",
    "@types/node": "20.17.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "8.11.0",
    "@typescript-eslint/parser": "8.11.0",
    "dotenv": "16.4.5",
    "eslint": "9.13.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "graphql": "16.9.0",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.12.4",
    "prettier": "3.3.3",
    "stylelint": "16.10.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recess-order": "5.1.1",
    "stylelint-config-standard": "36.0.1",
    "typescript": "5.6.3"
  }
}
