{
  "name": "trending-viwer",
  "private": true,
  "scripts": {
    "build": "parcel build index.html",
    "start": "parcel index.html --open",
    "lint": "yarn eslint",
    "format": "yarn eslint --fix && yarn stylelint --fix && yarn prettier --write",
    "eslint": "eslint --cache './**/*.{ts,tsx}'",
    "prettier": "prettier './**/*.{md,js,ts,tsx,json}'",
    "stylelint": "stylelint --cache --syntax css-in-js './src/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "devDependencies": {
    "@types/node": "12.0.7",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.13.0",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.12.3",
    "prettier": "1.18.2",
    "stylelint": "10.1.0",
    "stylelint-config-prettier": "5.2.0",
    "stylelint-config-recess-order": "2.0.2",
    "stylelint-config-standard": "18.3.0",
    "typescript": "3.5.1"
  }
}
