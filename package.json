{
  "name": "graphql-starred-repositories",
  "private": true,
  "scripts": {
    "build": "parcel build index.html",
    "start": "parcel index.html --open",
    "lint": "yarn eslint",
    "format": "yarn eslint --fix && yarn stylelint --fix && yarn prettier --write",
    "eslint": "eslint --cache './**/*.{ts,tsx}'",
    "prettier": "prettier './**/*.{md,js,ts,tsx,json}'",
    "stylelint": "stylelint --cache --syntax css-in-js './src/**/*.{ts,tsx}'",
    "graphql-codegen": "graphql-codegen --config codegen.yml --require dotenv/config",
    "postinstall": "yarn graphql-codegen"
  },
  "dependencies": {
    "graphql-tag": "2.10.1",
    "lens.ts": "https://github.com/tbo/lens.ts#master",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "urql": "1.5.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.4.0",
    "@graphql-codegen/fragment-matcher": "1.4.0",
    "@graphql-codegen/introspection": "1.4.0",
    "@graphql-codegen/typescript": "1.4.0",
    "@graphql-codegen/typescript-operations": "1.4.0",
    "@graphql-codegen/typescript-urql": "1.4.0",
    "@types/node": "12.6.6",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@typescript-eslint/eslint-plugin": "1.12.0",
    "@typescript-eslint/parser": "1.12.0",
    "dotenv": "8.0.0",
    "eslint": "6.0.1",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.2",
    "eslint-plugin-react-hooks": "1.6.1",
    "graphql": "14.4.2",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.12.3",
    "prettier": "1.18.2",
    "stylelint": "10.1.0",
    "stylelint-config-prettier": "5.2.0",
    "stylelint-config-recess-order": "2.0.3",
    "stylelint-config-standard": "18.3.0",
    "typescript": "3.5.3"
  }
}
